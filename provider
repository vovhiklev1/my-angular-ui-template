(function () {
    'use strict';

    angular.module('app').config(config);
    config.$inject = ['epicFactoryProvider'];
    function config(epicFactoryProvider) {
        epicFactoryProvider.setData(' + edited ')
    }
})();

(function () {
    'use strict';
    angular.module('app').provider('epicFactory', epicFactory);
    function epicFactory() {
        var data = 'epicFactory data';
        return {
            setData: function (val) {
                data += val;
            },
            $get: function () {
                return data
            }
        }
    }
})();

